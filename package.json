{
  "name": "essential-algorithms",
  "version": "1.0.0",
  "description": "Collection of essential algorithms you must know",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js problems/.* --watchAll --seed=0",
    "test:solutions": "node --experimental-vm-modules node_modules/jest/bin/jest.js problems/.* --seed=1",
    "test:ds": "node --experimental-vm-modules node_modules/jest/bin/jest.js data-structures/.* --watch",
    "watch:solutions": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --seed=1",
    "new:problem": "node new-problem.js",
    "prepare": "husky"
  },
  "jest": {
    "transform": {},
    "setupFilesAfterEnv": [
      "<rootDir>/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faizmaricar/essential-algorithms.git"
  },
  "author": "Faiz Maricar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/faizmaricar/essential-algorithms/issues"
  },
  "homepage": "https://github.com/faizmaricar/essential-algorithms#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "husky": "^9.1.4",
    "jest": "^29.7.0"
  }
}
